                            <a id="btn_search_tracking" class="btn btn-primary mb-4">Cerca</a>
                        </div>
                    </form>

                </div>

                <!-- Risultato di ricerca -->
                <div id="body-tracking" class="d-none">


                    <!-- NEW TEMPLATE -->

